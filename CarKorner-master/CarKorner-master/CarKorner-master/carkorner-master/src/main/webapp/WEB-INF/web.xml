<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchCarServlet</servlet-name>
  	<display-name>SearchCarServlet</display-name>
  	<description></description>
  	<servlet-class>com.dishtech.frontController.SearchCarServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ModelDetailsServlet</servlet-name>
  	<display-name>ModelDetailsServlet</display-name>
  	<description></description>
  	<servlet-class>com.dishtech.frontController.ModelDetailsServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SignUpServlet</servlet-name>
  	<display-name>SignUpServlet</display-name>
  	<description></description>
  	<servlet-class>com.dishtech.frontController.SignUpServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.dishtech.frontController.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SellBuyServlet</servlet-name>
  	<display-name>SellBuyServlet</display-name>
  	<description></description>
  	<servlet-class>com.dishtech.frontController.SellBuyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SearchCarServlet</servlet-name>
  	<url-pattern>/SearchCarServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ModelDetailsServlet</servlet-name>
  	<url-pattern>/ModelDetailsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignUpServlet</servlet-name>
  	<url-pattern>/SignUpServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SellBuyServlet</servlet-name>
  	<url-pattern>/SellBuyServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  <welcome-file>screens/login.jsp</welcome-file>
  </welcome-file-list>
</web-app>